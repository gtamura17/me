{
  "buildCommand": "npm run build:vercel",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "rewrites": [
    {
      "source": "/pt/(.*)",
      "destination": "/pt/$1"
    },
    {
      "source": "/en/(.*)",
      "destination": "/en/$1"
    },
    {
      "source": "/es/(.*)",
      "destination": "/es/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/pt/$1"
    }
  ]
}
